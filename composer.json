{
    "name": "laradic/support",
    "description": "Laravel support package",
    "homepage": "https://radic.io",
    "keywords": ["laravel", "laradic", "support", "repositories", "structure"],
    "license": "MIT",
    "authors": [
        {"name": "Robin Radic", "homepage": "https://radic.dev", "email": "rradic@hotmail.com", "role": "Developer"}
    ],
    "require": {
        "adbario/php-dot-notation": "~2.2",
        "webmozart/path-util": "~2.3",
        "symfony/expression-language": "~3.0|~4.0",
        "danielstjules/stringy": "~3.1.0",
        "gabrielelana/byte-units": "~0.5"
    },
    "require-dev": {
        "tcb13/substringy": "^1.0",
        "danielstjules/sliceable-stringy": "dev-master@dev",
        "friendsofphp/php-cs-fixer": "~2.0",
        "flow/jsonpath": "~0.4"
    },
    "suggest": {
        "tcb13/substringy": "^1.0",
        "danielstjules/sliceable-stringy": "dev-master@dev",
        "flow/jsonpath": "Enables JSONPath for arrays, collections and the wrapper"
    },
    "autoload": {
        "files": [
            "src/helpers.php",
            "src/path_helpers.php",
            "src/Helpers/Bytes.php",
            "src/Helpers/Dot.php",
            "src/Helpers/Stringy.php"
        ],
        "psr-4": {
            "Laradic\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradic\\Tests\\Support\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev",
            "dev-develop": "3.x-dev"
        }
    }
}
