{
    "name": "laradic/support",
    "description": "Laravel 5 support package",
    "homepage": "https://la.radic.nl",
    "keywords": ["laravel", "laradic", "support", "repositories", "structure"],
    "license": "MIT",
    "authors": [
        {"name": "Robin Radic", "homepage": "http://robin.radic.nl", "email": "robin@radic.nl", "role": "Developer"}
    ],
    "require": {
        "php": ">=7.1.0",
        "danielstjules/stringy": "~1.8|~2.0",
        "anahkiasen/underscore-php": "~2.0",
        "illuminate/support": "~5.0",
        "gabrielelana/byte-units": "0.2.*",
        "webmozart/assert": "1.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.7.*",
        "phpunit/phpunit": "~4.0|~5.0|~6.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Laradic\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradic\\Tests\\Support\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    }
}
